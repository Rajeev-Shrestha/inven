

@{
    ViewBag.Title = "Sales";
    Layout = "_Layout";
}
@section Scripts{
    <script src="~/js/sales/app-sales.js" asp-append-version="true"></script>
    <script src="~/js/sales/sales.controller.js" asp-append-version="true"></script>
    <script src="~/js/sales/sales.service.js" asp-append-version="true"></script>
    <script src="~/manual/angularSortable.js"></script>
    <script src="~/manual/paging.min.js"></script>
    <script src="~/lib/angular-moment/moment.js"></script>
    <script src="~/lib/angular-sanitize/angular-sanitize.js"></script>
<script type="text/javascript">
    angular.bootstrap(document.getElementById("sales"), ['app-sales']);
</script>
}
@section css{
    <link href="~/manual/icon.css" rel="stylesheet" />
}
<div ng-non-bindable>
    <div class="" id="sales">
        <ui-view></ui-view>
    </div>
</div>
